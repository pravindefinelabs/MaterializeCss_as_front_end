<% @courses.each do |course|%>
  <div class="col s6 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%=course.short_name %> : <%= course.name%> </span>
        <p><%= course.description%></p>
      </p>
    </div>
    <div class="card-action">
      <a href="#">Enroll</a>
      <a href="#">Info</a>
    </div>
  </div>
</div>
<% end%>